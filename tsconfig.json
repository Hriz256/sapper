{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "target": "es5",
    "module": "commonjs",
    "alwaysStrict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "forceConsistentCasingInFileNames": true,
//    "noUnusedLocals": true,
//    "noUnusedParameters": true
  }
}

